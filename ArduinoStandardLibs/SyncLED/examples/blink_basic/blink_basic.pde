#include <SyncLED.h>

// Create an LED instance on pin 13 (with default settings)
SyncLED StatusLight(13);

void setup() {
	// No setup required here
}

void loop() {

  // Turn the light on
  StatusLight.On();
  // Wait some time
  delay(400);
  // Turn the light off
  StatusLight.Off();
  // Wait some more time
  delay(200);
}